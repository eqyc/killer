        "parameters": [
          {
            "name": "WERKS",
            "description": "工厂",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "MATNR",
            "description": "物料号",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {

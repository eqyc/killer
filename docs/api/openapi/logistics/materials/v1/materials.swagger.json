            "in": "query",
            "required": false,
            "type": "string",
            "format": "date-time"
          },
          {
            "name": "reverseReason",
            "description": "冲销原因",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "MaterialsService"
        ]
      }
    }

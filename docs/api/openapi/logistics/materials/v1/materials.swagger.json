  "paths": {
    "/v1-materials/materialdocument": {
      "post": {
        "summary": "获取物料凭证",
        "operationId": "MaterialsService_GetMaterialDocument",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1GetMaterialDocumentResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "MBLNR",
            "description": "物料凭证号",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "MJAHR",
            "description": "物料凭证年度",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          }
        ],
        "tags": [
          "MaterialsService"
        ]
      }
    },
    "/v1-materials/materialdocuments": {
      "post": {
        "summary": "查询物料凭证列表",

        }
      },
      "title": "创建主数据记录响应"
    },
    "v1GetMasterRecordResponse": {
      "type": "object",
      "properties": {
        "masterRecord": {
          "$ref": "#/definitions/v1MasterRecord",
          "title": "主数据记录"
        }
      },
      "title": "获取主数据记录响应"
    },
    "v1ListMasterRecordsResponse": {
      "type": "object",
      "properties": {
        "masterRecords": {
          "type": "array",
          "items": {
            "type": "object",

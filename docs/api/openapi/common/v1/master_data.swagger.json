        },
        "DESCRIPTION": {
          "type": "string",
          "title": "描述"
        },
        "ATTRIBUTES": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          },
          "title": "属性集合（键值对）"
        },
        "CREQUESTID": {
          "type": "string",
          "title": "变更请求号，对应CREQUEST_ID"
        },
        "STATUS": {
          "type": "string",
          "title": "状态（草稿/待审批/已激活/已废弃）"
        },
        "SOURCESYSTEM": {
          "type": "string",
          "title": "数据来源"
        },
        "VERSION": {
          "type": "integer",
          "format": "int32",
          "title": "版本号"
        },
        "VALIDFROM": {
          "type": "string",
          "format": "date-time",
          "title": "有效起始日期"
        },
        "VALIDTO": {
          "type": "string",
          "format": "date-time",
          "title": "有效截止日期"
        },
        "BUKRS": {
          "type": "string",
          "title": "公司代码"
        },
        "WERKS": {
          "type": "string",
          "title": "工厂"
        },
        "ORGDATA": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          },
          "title": "组织层级数据"
        },
        "LOEKZ": {
          "type": "string",
          "title": "删除标识，对应LOEKZ"
        },
        "auditInfo": {
          "$ref": "#/definitions/v1AuditInfo",
          "title": "审计信息"
        },
        "fields": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/v1MasterRecordField"
          },
          "title": "扩展字段列表"
        }
      },
      "title": "=============================================================================\nMasterRecord - 主数据记录（聚合根）\n=============================================================================\n对应MDG通用主数据模型"
    },
    "v1MasterRecordField": {
      "type": "object",
      "properties": {
        "FIELDNAME": {
          "type": "string",
          "title": "字段名称"
        },
        "FIELDVALUE": {
          "type": "string",
          "title": "字段值"
        },
        "FIELDTYPE": {
          "type": "string",
          "title": "字段类型（字符串/数字/日期等）"
        },
        "FIELDDESC": {
          "type": "string",
          "title": "字段描述"
        },
        "REQUIRED": {
          "type": "boolean",
          "title": "是否必填"
        },
        "VALIDATIONRULE": {
          "type": "string",
          "title": "验证规则"
        },
        "ORGLEVEL": {
          "type": "string",
          "title": "组织级别"
        }
      },
      "title": "=============================================================================\nMasterRecordField - 主数据字段\n============================================================================="

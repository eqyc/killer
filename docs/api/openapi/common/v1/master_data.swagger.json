{
  "swagger": "2.0",
  "info": {
    "title": "common/v1/master_data.proto",
    "version": "version not set"
  },
  "tags": [
    {
      "name": "MasterDataService"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1-master_data/masterrecord": {
      "post": {
        "summary": "获取主数据记录",
        "operationId": "MasterDataService_GetMasterRecord",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1GetMasterRecordResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "RECORDID",
            "description": "记录标识",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "TYPE",
            "description": "或通过类型+对象编号查询",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "OBJECTID",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "effectiveDate",
            "description": "有效日期",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "date-time"
          }
        ],
        "tags": [
          "MasterDataService"
        ]
      }
    },
    "/v1-master_data/masterrecords": {
      "post": {
        "summary": "查询主数据记录列表",
        "operationId": "MasterDataService_ListMasterRecords",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1ListMasterRecordsResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "TYPE",
            "description": "主数据类型",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "STATUS",
            "description": "状态",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "BUKRS",
            "description": "公司代码",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "WERKS",
            "description": "工厂",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "SOURCESYSTEM",
            "description": "数据来源",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "pageRequest.pageNumber",
            "description": "页码（从1开始）",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "pageRequest.pageSize",
            "description": "每页大小（默认20，最大100）",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "pageRequest.sortBy",
            "description": "排序字段名",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "pageRequest.sortOrder",
            "description": "排序方向\n\n - SORT_ORDER_UNSPECIFIED: 未指定\n - SORT_ORDER_ASC: 升序\n - SORT_ORDER_DESC: 降序",
            "in": "query",
            "required": false,
            "type": "string",
            "enum": [
              "SORT_ORDER_UNSPECIFIED",
              "SORT_ORDER_ASC",
              "SORT_ORDER_DESC"
            ],
            "default": "SORT_ORDER_UNSPECIFIED"
          }
        ],
        "tags": [
          "MasterDataService"
        ]
      }
    },
    "/v1-master_data/masterrecordstatus": {
      "post": {
        "summary": "更新主数据状态",
        "operationId": "MasterDataService_UpdateMasterRecordStatus",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1UpdateMasterRecordStatusResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "RECORDID",
            "description": "记录标识",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "STATUS",
            "description": "新状态",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "NOTES",
            "description": "备注",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "MasterDataService"
        ]
      }
    }
  },
